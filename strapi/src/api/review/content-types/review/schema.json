{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "reviewer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user.user"
    },
    "submission": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::submission.submission",
      "inversedBy": "review"
    },
    "reviewComments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review-comment.review-comment",
      "mappedBy": "review"
    }
  }
}

    "hints": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::hint.hint",
      "mappedBy": "problem_id"
    },
    "initial_codes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::initial-code.initial-code",
      "mappedBy": "problem_id"
    },
    "test_cases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::test-case.test-case",

{
  "kind": "collectionType",
  "collectionName": "problems",
  "info": {
    "displayName": "Problem",
    "singularName": "problem",
    "pluralName": "problems"
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "testcases": {
      "type": "json"
    },
    "parsers": {
      "type": "json"
    },
    "initialCodes": {
      "type": "json"
    },
    "modelCodes": {
      "type": "json"
    },
    "isDraft": {
      "type": "boolean"
    },
    "solutions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::solution.solution",
      "mappedBy": "problem"
    }
  },
  "options": {
    "draftAndPublish": false
  }
}
